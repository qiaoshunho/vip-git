<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!--提供方信息-->
    <dubbo:application name="test-server" owner="qiaoshunho"/>
    <!--注册中心-->
    <dubbo:registry address="zookeeper://192.168.131.129:2181?backup=192.168.131.130:2181,192.168.131.131:2181"/>

    <dubbo:protocol port="20880" name="dubbo"/>

    <dubbo:service interface="com.ultra.IHello"
                   ref="helloService"  />

    <bean id="helloService" class="com.ultra.HelloImpl"/>
       
</beans>
